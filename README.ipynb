    "import gym\n",
    "import gym_anytrading\n",
    "\n",
    "env = gym.make('forex-v0')\n",
    "# env = gym.make('stocks-v0')\n"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
